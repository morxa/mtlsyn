disjunction {
  disjuncts {
    conjunction {
      conjuncts {
        until {
          front {
            negation {
             formula { atomic { symbol: "finish_close" } }
            }
          }
          back {
            atomic { symbol: "enter" }
          }
        }
      }
      conjuncts {
        finally { formula { atomic { symbol: "enter" } }}
      }
    }
  }
  disjuncts {
    until {
      front {
        negation {
         formula { atomic { symbol: "finish_open" } }
        }
      }
      back {
        atomic { symbol: "disappear" }
      }
    }
  }
}
